time:xsdDateTime a owl:DatatypeProperty,
        owl:DeprecatedProperty ;
    rdfs:label "has XSD date-time"@en,
        "tiene fecha-hora XSD"@es ;
    rdfs:comment "Value of DateTimeInterval expressed as a compact value."@en,
        "Valor de 'intervalo de fecha-hora' expresado como un valor compacto."@es ;
    rdfs:domain time:DateTimeInterval ;
    rdfs:range xsd:dateTime ;
    owl:deprecated true ;
    skos:note "Utilizando xsd:dateTime en este lugar significa que la duración del intervalo está implícita: se corresponde con la longitud del elemento más pequeño distinto de cero del literal fecha-hora. Sin embargo, esta regla no se puede utilizar para intervalos cuya duración es mayor que un rango más pequeño que el tiempo de comienzo - p.ej. el primer minuto o segundo del día, la primera hora del mes, o el primer día del año. En estos casos el intervalo deseado no se puede distinguir del intervalo correspondiente al próximo rango más alto. Debido a esta ambigüedad esencial, no se recomienda el uso de esta propiedad y está desaprobada.",
        "Using xsd:dateTime in this place means that the duration of the interval is implicit: it corresponds to the length of the smallest non-zero element of the date-time literal. However, this rule cannot be used for intervals whose duration is more than one rank smaller than the starting time - e.g. the first minute or second of a day, the first hour of a month, or the first day of a year. In these cases the desired interval cannot be distinguished from the interval corresponding to the next rank up. Because of this essential ambiguity, use of this property is not recommended and it is deprecated."@en .

<https://w3id.org/catrisk/ontology> a owl:Ontology ;
    rdfs:label "Catastrophe Risk Ontology" ;
    rdfs:comment "An ontology to describe key concepts and relationships in catastrophe property insurance modeling, including exposure, events, losses, perils, and portfolios." ;
    dcterms:contributor [ a sdo:Person ;
            sdo:affiliation "NFP, Inc." ;
            sdo:email "tadinadasashi@gmail.com"^^xsd:string ;
            sdo:name "Sashi Kanth Tadinada, PhD" ] ;
	dcterms:created "2025-08-07"^^xsd:date ;
	vann:preferredNamespacePrefix "cro"^^xsd:string ;
	vann:preferredNamespaceUri "https://w3id.org/catrisk/ontology#"^^xsd:anyURI ;
	owl:versionIRI <https://w3id.org/catrisk/ontology/1.0.0> .
	

cro:AccountAAL rdfs:label "Account AAL" ;
    rdfs:comment "Account Average Annual Loss from cat modeling" ;
    rdfs:subClassOf cro:AAL .

cro:AreaCode rdfs:label "Area Code" ;
    rdfs:comment "A code that uniquely identifies a geographic subregion or administrative boundary used in exposure or loss modeling." ;
    rdfs:subClassOf cro:AdminBoundary .

cro:County rdfs:label "County" ;
    rdfs:comment "County" ;
    rdfs:subClassOf cro:AdminBoundary .

cro:IBNR a rdf:Property,
        owl:FunctionalProperty ;
    rdfs:label "Incurred But Not Reported" ;
    rdfs:comment "Estimate of claims incurred but not yet reported for the event." ;
    rdfs:domain cro:EventClaim ;
    rdfs:range xsd:float .

cro:LocationAAL rdfs:label "Location AAL" ;
    rdfs:comment "Location AAL from cat model analysis" ;
    rdfs:subClassOf cro:AAL .

cro:LossModeling rdfs:label "Loss Modeling" ;
    rdfs:comment "The process and configuration used to simulate losses from exposures given a hazard scenario, model version, and financial terms." .

cro:PerilGroup rdfs:label "Peril Group" ;
    rdfs:comment "Set of perils grouped together (Examples: Peril Group of \"Meteorological Disasters\" could mean tropical cyclone, extra-tropical storms and convective storms)" ;
    rdfs:subClassOf cro:Peril .

cro:PortfolioAAL rdfs:label "Portfolio AAL" ;
    rdfs:comment "Portfolio Average Annual Loss" ;
    rdfs:subClassOf cro:AAL .

cro:ScenarioModelingRecord rdfs:label "Scenario Modeling Record" ;
    rdfs:comment "Exposure accumulation or loss output corresponding to a scenario modeling analysis" ;
    rdfs:subClassOf cro:AnalysisOutput .

cro:ZipCode rdfs:label "Postal Code" ;
    rdfs:comment "Postal Code" ;
    rdfs:subClassOf cro:AdminBoundary .

cro:adminBoundaryCategory a rdf:Property,
        owl:FunctionalProperty ;
    rdfs:label "Admin Boundary Category" ;
    rdfs:comment "Type of administrative boundary, such as country, state, province, or district." ;
    rdfs:domain cro:Country ;
    rdfs:range xsd:string .

cro:adminBoundaryCode a rdf:Property,
        owl:FunctionalProperty ;
    rdfs:label "Admin Boundary Code" ;
    rdfs:comment "A code identifying the administrative boundary, often ISO country code or internal system code." ;
    rdfs:domain cro:Country ;
    rdfs:range xsd:string .

cro:adminBoundaryContains a rdf:Property ;
    rdfs:label "Contains Administrative Boundary" ;
    rdfs:comment "Indicates that the administrative boundary contains or includes another administrative boundary (e.g., a state contains counties)." ;
    rdfs:domain cro:AdminBoundary ;
    rdfs:range cro:AdminBoundary .

cro:adminBoundaryName a rdf:Property,
        owl:FunctionalProperty ;
    rdfs:label "Admin Boundary Name" ;
    rdfs:comment "The name of the administrative boundary (e.g., \"United States\", \"California\")." ;
    rdfs:domain cro:Country ;
    rdfs:range xsd:string .

cro:adminBoundarylevel a rdf:Property,
        owl:FunctionalProperty ;
    rdfs:label "Administrative Boundary Level" ;
    rdfs:comment "An integer representing the hierarchical level of the administrative boundary (e.g., 0 for country, 1 for state, 2 for county)." ;
    rdfs:domain cro:AdminBoundary ;
    rdfs:range xsd:integer .

cro:analysisDate a rdf:Property,
        owl:FunctionalProperty ;
    rdfs:label "Analysis Date" ;
    rdfs:comment "Date and time when the loss modeling analysis was performed." ;
    rdfs:domain cro:LossModelAnalysis ;
    rdfs:range time:Instant .

cro:analysisDescription a rdf:Property,
        owl:FunctionalProperty ;
    rdfs:label "Analysis Description" ;
    rdfs:comment "A summary description of the catastrophe modeling analysis performed, including scope and assumptions." ;
    rdfs:domain cro:LossModelAnalysis ;
    rdfs:range xsd:string .

cro:analysisID a rdf:Property,
        owl:FunctionalProperty ;
    rdfs:label "Analysis ID" ;
    rdfs:comment "Unique identifier assigned to the loss modeling analysis instance." ;
    rdfs:domain cro:LossModelAnalysis ;
    rdfs:range xsd:string .

cro:analysisLevel a rdf:Property,
        owl:FunctionalProperty ;
    rdfs:label "Analysis Level" ;
    rdfs:comment "The granularity level of the analysis (e.g., portfolio level, account level, location level)." ;
    rdfs:domain cro:LossModelAnalysis ;
    rdfs:range xsd:string .

cro:analysisName a rdf:Property,
        owl:FunctionalProperty ;
    rdfs:label "Analysis Name" ;
    rdfs:comment "Name or label given to this specific loss modeling analysis." ;
    rdfs:domain cro:LossModelAnalysis ;
    rdfs:range xsd:string .

cro:analysisType a rdf:Property,
        owl:FunctionalProperty ;
    rdfs:label "Analysis Type" ;
    rdfs:comment "Analysis Type to represent if probabilistic loss modeling or accumulation loss modeling " ;
    rdfs:domain cro:LossModelAnalysis ;
    rdfs:range xsd:string .

cro:availabilityInterval a rdf:Property,
        owl:FunctionalProperty ;
    rdfs:label "Availability Interval" ;
    rdfs:comment "The time interval during which the catastrophe model version is available for use, typically referencing its validity window for regulatory or client use." ;
    rdfs:domain cro:CatastropheModel ;
    rdfs:range time:Interval .

cro:catDomainDescription a rdf:Property,
        owl:FunctionalProperty ;
    rdfs:label "Cat Domain Description" ;
    rdfs:comment "Textual description of the catastrophe model’s domain region, defining its geographic scope." ;
    rdfs:domain cro:CatModelDomainRegion ;
    rdfs:range xsd:string .

cro:catFamily a rdf:Property,
        owl:FunctionalProperty ;
    rdfs:label "Catastrophe Family" ;
    rdfs:comment "A grouping of related catastrophe event types (e.g., Tropical Cyclone, Earthquake)." ;
    rdfs:domain cro:Event ;
    rdfs:range xsd:string .

cro:catModelAvailabilityEnd a rdf:Property,
        owl:FunctionalProperty ;
    rdfs:label "Availability End Year" ;
    rdfs:comment "The ending year of availability for the catastrophe model version." ;
    rdfs:domain cro:CatastropheModel ;
    rdfs:range xsd:int .

cro:catModelAvailabilityStart a rdf:Property,
        owl:FunctionalProperty ;
    rdfs:label "Availability Start Year" ;
    rdfs:comment "The starting year of availability for the catastrophe model version." ;
    rdfs:domain cro:CatastropheModel ;
    rdfs:range xsd:int .

cro:catModelDescription a rdf:Property,
        owl:FunctionalProperty ;
    rdfs:label "Model Description" ;
    rdfs:comment "A textual description providing high-level information about the catastrophe model, including its scope, version, and intended application." ;
    rdfs:domain cro:CatastropheModel ;
    rdfs:range xsd:string .

cro:catModelEvaluationNotes a rdf:Property,
        owl:FunctionalProperty ;
    rdfs:label "Model Evaluation Notes" ;
    rdfs:comment "Notes capturing evaluation results, validation, and model review commentary used for internal governance or regulatory reporting." ;
    rdfs:domain cro:CatastropheModel ;
    rdfs:range xsd:string .

cro:catModelID a rdf:Property,
        owl:FunctionalProperty ;
    rdfs:label "Model Identifier" ;
    rdfs:comment "A unique identifier assigned to the catastrophe model version, typically used for reference across systems." ;
    rdfs:domain cro:CatastropheModel ;
    rdfs:range xsd:string .

cro:catModelName a rdf:Property,
        owl:FunctionalProperty ;
    rdfs:label "Model Name" ;
    rdfs:comment "The official name of the catastrophe model, typically indicating vendor and peril (e.g., \"RMS North Atlantic Hurricane 2023\")." ;
    rdfs:domain cro:CatastropheModel ;
    rdfs:range xsd:string .

cro:catModelPeril a rdf:Property,
        owl:FunctionalProperty ;
    rdfs:label "Model Peril Description" ;
    rdfs:comment "A textual representation of the peril(s) modeled by this catastrophe model." ;
    rdfs:domain cro:CatastropheModel ;
    rdfs:range xsd:string .

cro:catModelRegion a rdf:Property,
        owl:FunctionalProperty ;
    rdfs:label "Model Region Description" ;
    rdfs:comment "A textual representation of the geographic region covered by this catastrophe model." ;
    rdfs:domain cro:CatastropheModel ;
    rdfs:range xsd:string .

cro:claimPartOf a rdf:Property,
        owl:FunctionalProperty ;
    rdfs:label "Claim Part Of" ;
    rdfs:comment "Links the policy claim to the broader event claim record it belongs to." ;
    rdfs:domain cro:PolicyClaim ;
    rdfs:range cro:EventClaim .

cro:completenessRatio a rdf:Property,
        owl:FunctionalProperty ;
    rdfs:label "Completeness Ratio" ;
    rdfs:comment "Ratio indicating the completeness of claim reporting for the event." ;
    rdfs:domain cro:EventClaim ;
    rdfs:range xsd:float .

cro:coveredBy a rdf:Property,
        owl:InverseFunctionalProperty ;
    rdfs:label "Covered By Policy" ;
    rdfs:comment "Indicates which insurance policy or policies provide coverage for the exposures grouped within the account." ;
    rdfs:domain cro:Account ;
    rdfs:range cro:Policy .

cro:detailLevel a rdf:Property,
        owl:FunctionalProperty ;
    rdfs:label "Detail Level" ;
    rdfs:comment "Indicates the granularity of this result record (summary or detailed)." ;
    rdfs:domain cro:CatModelResultRecord ;
    rdfs:range xsd:string .

cro:ep_loss a rdf:Property,
        owl:FunctionalProperty ;
    rdfs:label "EP Loss" ;
    rdfs:comment "The loss value corresponding to the given exceedance probability return period." ;
    rdfs:domain cro:EPCURVE ;
    rdfs:range xsd:float .

cro:ep_loss_sd a rdf:Property,
        owl:FunctionalProperty ;
    rdfs:label "EP Loss Standard Deviation" ;
    rdfs:comment "The standard deviation of loss at the given exceedance probability return period." ;
    rdfs:domain cro:EPCURVE ;
    rdfs:range xsd:float .

cro:epindex a rdf:Property,
        owl:FunctionalProperty ;
    rdfs:label "EP Index" ;
    rdfs:comment "Index or reference used in the exceedance probability mapping of event and peril." ;
    rdfs:domain cro:EventPerilMap ;
    rdfs:range xsd:string .

cro:eventID a rdf:Property,
        owl:FunctionalProperty ;
    rdfs:label "Event ID" ;
    rdfs:comment "Unique identifier for the catastrophe event within the model or database." ;
    rdfs:domain cro:Event ;
    rdfs:range xsd:string .

cro:eventName a rdf:Property,
        owl:FunctionalProperty ;
    rdfs:label "Event Name" ;
    rdfs:comment "The name of the catastrophe event (may be scientific or commonly known name)." ;
    rdfs:domain cro:Event ;
    rdfs:range xsd:string .

cro:eventQuarter a rdf:Property,
        owl:FunctionalProperty ;
    rdfs:label "Event Quarter" ;
    rdfs:comment "The calendar quarter when the event occurred (e.g., Q1, Q2)." ;
    rdfs:domain cro:Event ;
    rdfs:range xsd:string .

cro:eventYear a rdf:Property,
        owl:FunctionalProperty ;
    rdfs:label "Event Year" ;
    rdfs:comment "The year in which the event occurred." ;
    rdfs:domain cro:Event ;
    rdfs:range xsd:string .

cro:eventinfo a rdf:Property,
        owl:FunctionalProperty ;
    rdfs:label "Event Info" ;
    rdfs:comment "Additional information or notes about the event within the peril map." ;
    rdfs:domain cro:EventPerilMap ;
    rdfs:range xsd:string .

cro:exposureDetailLevel a rdf:Property,
        owl:FunctionalProperty ;
    rdfs:label "Exposure Detail Level" ;
    rdfs:comment "Indicates the level of detail in the exposure data for the portfolio." ;
    rdfs:domain cro:Portfolio ;
    rdfs:range xsd:string .

cro:exposureFor a rdf:Property,
        owl:InverseFunctionalProperty ;
    rdfs:label "Exposure For" ;
    rdfs:comment "Links the exposure record to the associated result record for analysis." ;
    rdfs:domain cro:Exposure ;
    rdfs:range cro:CatModelResultRecord .

cro:factor a rdf:Property,
        owl:FunctionalProperty ;
    rdfs:label "Factor" ;
    rdfs:comment "A factor value used in scaling or mapping the peril contribution for this event." ;
    rdfs:domain cro:EventPerilMap ;
    rdfs:range xsd:float .

cro:forFeature a rdf:Property,
        owl:FunctionalProperty ;
    rdfs:label "For Feature" ;
    rdfs:comment "Indicates which feature (e.g., location or risk characteristic) is involved in accumulation analysis." ;
