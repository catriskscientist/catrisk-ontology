@prefix cro: <https://w3id.org/catrisk/ontology#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix geo: <http://www.opengis.net/ont/geosparql#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix sdo: <https://schema.org/> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix spec11: <http://www.opengis.net/spec/geosparql/1.1/specification.html#> .
@prefix time: <http://www.w3.org/2006/time#> .
@prefix vann: <http://purl.org/vocab/vann/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

geo:Feature a rdfs:Class,
        owl:Class ;
    rdfs:isDefinedBy geo:,
        <http://www.opengis.net/spec/geosparql/1.0/req/core/feature-class>,
        <http://www.opengis.net/spec/geosparql/1.1/req/core/feature-class> ;
    rdfs:subClassOf geo:SpatialObject ;
    owl:disjointWith geo:Geometry ;
    skos:definition "A discrete spatial phenomenon in a universe of discourse."@en ;
    skos:example spec11:C.1.1.2.1,
        spec11:C.1.1.2.2,
        spec11:C.1.1.2.3,
        spec11:C.1.1.2.4,
        spec11:C.1.1.2.5,
        spec11:C.1.1.2.6,
        spec11:C.1.1.2.7,
        spec11:C.1.1.2.8,
        spec11:C.1.1.2.9,
        spec11:C.1.1.3.2,
        spec11:C.1.1.3.3,
        spec11:C.1.2.2,
        spec11:C.1.2.3,
        spec11:C.1.2.4 ;
    skos:note "A Feature represents a uniquely identifiable phenomenon, for example a river or an apple. While such phenomena (and therefore the Features used to represent them) are bounded, their boundaries may be crisp (e.g., the declared boundaries of a state), vague (e.g., the delineation of a valley versus its neighboring mountains), and change with time (e.g., a storm front). While discrete in nature, Features may be created from continuous observations, such as an isochrone that determines the region that can be reached by ambulance within 5 minutes."@en ;
    skos:prefLabel "Feature"@en .

geo:FeatureCollection a rdfs:Class,
        owl:Class ;
    rdfs:isDefinedBy geo:,
        <http://www.opengis.net/spec/geosparql/1.1/req/core/feature-collection-class> ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom geo:Feature ;
            owl:onProperty rdfs:member ],
        geo:SpatialObjectCollection ;
    skos:definition "A collection of individual Features."@en ;
    skos:example spec11:C.1.1.6 ;
    skos:prefLabel "Feature Collection"@en .

geo:Geometry a rdfs:Class,
        owl:Class ;
    rdfs:isDefinedBy geo:,
        <http://www.opengis.net/spec/geosparql/1.0/req/geometry-extension/geometry-class>,
        <http://www.opengis.net/spec/geosparql/1.1/req/geometry-extension/geometry-class> ;
    rdfs:subClassOf geo:SpatialObject ;
    skos:definition "A coherent set of direct positions in space. The positions are held within a Spatial Reference System (SRS)."@en ;
    skos:example spec11:C.1.1.2.2,
        spec11:C.1.1.2.3,
        spec11:C.1.1.2.4,
        spec11:C.1.1.2.5,
        spec11:C.1.1.2.6,
        spec11:C.1.1.2.8,
        spec11:C.1.1.3.1,
        spec11:C.1.1.3.2,
        spec11:C.1.1.3.3,
        spec11:C.1.2.2,
        spec11:C.1.2.3,
        spec11:C.1.2.4 ;
    skos:note "Geometry can be used as a representation of the shape, extent or location of a Feature and may exist as a self-contained entity."@en ;
    skos:prefLabel "Geometry"@en .

geo:GeometryCollection a rdfs:Class,
        owl:Class ;
    rdfs:isDefinedBy geo:,
        <http://www.opengis.net/spec/geosparql/1.1/req/core/geometry-collection-class> ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom geo:Geometry ;
            owl:onProperty rdfs:member ],
        geo:SpatialObjectCollection ;
    skos:definition "A collection of individual Geometries."@en ;
    skos:example spec11:C.1.1.7 ;
    skos:prefLabel "Geometry Collection"@en .

geo:SpatialObject a rdfs:Class,
        owl:Class ;
    rdfs:isDefinedBy geo:,
        <http://www.opengis.net/spec/geosparql/1.0/req/core/spatial-object-class>,
        <http://www.opengis.net/spec/geosparql/1.1/req/core/spatial-object-class> ;
    skos:definition "Anything spatial (being or having a shape, position or an extent)."@en ;
    skos:example spec11:C.1.1.1.1,
        spec11:C.1.1.1.2 ;
    skos:note "Subclasses of this class are expected to be used for instance data."@en ;
    skos:prefLabel "Spatial Object"@en .

geo:SpatialObjectCollection a rdfs:Class,
        owl:Class ;
    rdfs:isDefinedBy geo:,
        <http://www.opengis.net/spec/geosparql/1.1/req/core/spatial-object-collection-class> ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom geo:SpatialObject ;
            owl:onProperty rdfs:member ],
        rdfs:Container ;
    skos:definition "A collection of individual Spatial Objects."@en ;
    skos:note "This is the superclass of Feature Collection and Geometry Collection."@en ;
    skos:prefLabel "Spatial Object Collection"@en .

cro:AAL a rdfs:Class ;
    rdfs:label "Average Annual Loss" ;
    rdfs:comment "The long-term expected loss per year calculated by catastrophe models. A key metric used in portfolio risk assessment and reinsurance pricing." ;
    rdfs:subClassOf cro:CatModelResultRecord .

cro:AEP a rdfs:Class ;
    rdfs:label "Aggregate EP" ;
    rdfs:comment "The exceedance probability curve representing the likelihood that the aggregate loss across all events exceeds various loss levels within a given period (typically a year)." ;
    rdfs:subClassOf cro:EPCURVE .

cro:Account a rdfs:Class ;
    rdfs:label "Insured Account" ;
    rdfs:comment "Represents a grouping of policies typically associated with a single insured or corporate client. Used for exposure roll-up, underwriting, and claims linkage." ;
    rdfs:subClassOf cro:Exposure .

cro:AdminBoundary a rdfs:Class ;
    rdfs:label "Administrative Boundary" ;
    rdfs:comment "Geographic boundaries such as country, state, or municipality used for regulatory reporting, exposure aggregation, and hazard mapping." ;
    rdfs:subClassOf geo:Feature .

cro:AggregationVector a rdfs:Class ;
    rdfs:label "Aggregation Vector" ;
    rdfs:comment "Class to define composite keys in order to aggregate results by (Example: Peril Region)" .

cro:AnalysisOutput a rdfs:Class ;
    rdfs:label "Analysis Output" ;
    rdfs:comment "Output objects from loss model analysis" .

cro:CatModelResultRecord a rdfs:Class ;
    rdfs:label "Catastrophe Model Result Record" ;
    rdfs:comment "Class representing cat modeling analysis output records such as AAL, EP curves recorded at various exposure resolutions such as location account, portfolio " ;
    rdfs:subClassOf cro:AnalysisOutput .

cro:CatastropheModel a rdfs:Class ;
    rdfs:label "Catastrophe Risk Model" ;
    rdfs:comment "A probabilistic model representing peril-specific hazard, vulnerability, and financial modules to estimate losses to insured portfolios from catastrophic events." .

cro:CatastropheModelingAnalysis a rdfs:Class ;
    rdfs:label "Catastrophe Modeling Analysis" ;
    rdfs:comment "The full set of outputs generated from a catastrophe model run, including EP curves, AAL, event losses, and portfolio-level metrics for exposure analysis." ;
    rdfs:subClassOf cro:LossModelAnalysis .

cro:CharacteristicEvent a rdfs:Class ;
    rdfs:label "Characteristic Event" ;
    rdfs:comment "Characteristic Events (CEs) are defined, high-probability events used in catastrophe modeling to assess and manage risk, particularly for large loss potential. Example: 100-year windstorm for TX and Gulf (More Details at https://www.karenclarkandco.com/solutions/characteristic-events)" ;
    rdfs:subClassOf cro:RiskScenario .

cro:DisasterScenario a rdfs:Class ;
    rdfs:label "Disaster Scenario" ;
    rdfs:comment "Disaster Scenario (example: Lloyd's Realistic Disaster Scenario) used for exposure accumulation" ;
    rdfs:subClassOf cro:RiskScenario .

cro:ELT a rdfs:Class ;
    rdfs:label "Event Loss Table" ;
    rdfs:comment "Table with list of simulated events, along with key information about each event and its associated losses (one of the key catastrophe modeling outputs)" ;
    rdfs:subClassOf cro:CatModelResultRecord .

cro:EPCURVE a rdfs:Class ;
    rdfs:label "Exceedance Probability Curve" ;
    rdfs:comment "A cumulative distribution function that shows the probability of exceeding various levels of loss. This is one of key outputs from the catastrophe models and represented as Loss expected to be exceeded at a Return Period (years) or exceedance probability. " ;
    rdfs:subClassOf cro:CatModelResultRecord .

cro:Event a rdfs:Class ;
    rdfs:label "Event" ;
    rdfs:comment "A modeled or historical hazard occurrence (e.g., hurricane, earthquake) with defined parameters such as footprint, intensity, and frequency." .

cro:EventClaim a rdfs:Class ;
    rdfs:label "Event Claim" ;
    rdfs:comment "A claim directly associated with a specific catastrophe event, capturing details like incurred losses, covered perils, and policy-level / line of business linkage." .

cro:EventFootprint a rdfs:Class ;
    rdfs:label "Event Footprint" ;
    rdfs:comment "Subclass of geo:FeatureCollection to represent event footprints" ;
    rdfs:subClassOf geo:FeatureCollection .

cro:EventFootprintGeometry a rdfs:Class ;
    rdfs:label "Event Footprint Geometry" ;
    rdfs:comment "Geometry of spatial footprint of an event" ;
    rdfs:subClassOf geo:GeometryCollection .

cro:EventLoss a rdfs:Class ;
    rdfs:label "Event Loss" ;
    rdfs:comment "Event Loss class caputures gross incurred loss for an event, peril, line of business and loss country combination" .

cro:EventPerilMap a rdfs:Class ;
    rdfs:label "Event Peril Mapping" ;
    rdfs:comment "Event Peril Map class maps events and constituent perils along with peril contribution factors " .

cro:Exposure a rdfs:Class ;
    rdfs:label "Exposure" ;
    rdfs:comment "Represents physical assets at risk, including their characteristics (e.g., location, construction, occupancy) used to model vulnerability and losses." .

cro:ExposureAccumulationAnalysis a rdfs:Class ;
    rdfs:label "Exposure Accumulation Analysis" ;
    rdfs:comment "The output of evaluating total exposures aggregated across defined regions, portfolios, or perils. Used to assess potential accumulation risk.	" ;
    rdfs:subClassOf cro:LossModelAnalysis .

cro:Grid a rdfs:Class ;
    rdfs:label "Grid" ;
    rdfs:comment "Region " ;
    rdfs:subClassOf geo:Feature .

cro:LOB a rdfs:Class ;
    rdfs:label "Line of Business" ;
    rdfs:comment "Insurance segmentation (e.g., Personal Property, Commercial Property, Marine) used for pricing, reporting, and risk diversification analysis." ;
    rdfs:subClassOf cro:Exposure .

cro:Location a rdfs:Class ;
    rdfs:label "Insured Location" ;
    rdfs:comment "A geocoded site of insured value, representing the lowest level of granularity in exposure modeling and the basis for hazard intersection." ;
    rdfs:subClassOf cro:Exposure .

cro:LossModelAnalysis a rdfs:Class ;
    rdfs:label "Loss Model Analysis" ;
    rdfs:comment "Generalized Class representing loss analyses - could be cat modeling or exposure accumulation" .

cro:OEP a rdfs:Class ;
    rdfs:label "Occurrence EP" ;
    rdfs:comment "The exceedance probability curve representing the likelihood that a single event exceeds various loss levels within a given period (typically a year)." ;
    rdfs:subClassOf cro:EPCURVE .

cro:Policy a rdfs:Class ;
    rdfs:label "Policy" ;
    rdfs:comment "A contractual agreement outlining the insured assets, coverages, deductibles, limits, and perils covered. Central to loss calculation and claims settlement." .

cro:PolicyClaim a rdfs:Class ;
    rdfs:label "Policy-Level Claim" ;
    rdfs:comment "The financial and administrative representation of a claim under a specific policy, including coverage validation, financials, and event linkages." .

cro:Portfolio a rdfs:Class ;
    rdfs:label "Portfolio" ;
    rdfs:comment "A collection of exposures or policies, typically aggregated for underwriting, capital allocation, or catastrophe modeling at a business unit level." ;
    rdfs:subClassOf cro:Exposure .

cro:Region a rdfs:Class ;
    rdfs:label "Region" ;
    rdfs:comment "A geographic area used for risk aggregation, exposure allocation, or loss estimation in catastrophe modeling.	" ;
    rdfs:subClassOf geo:FeatureCollection .

cro:RiskScenario a rdfs:Class ;
    rdfs:label "Risk Scenario" ;
    rdfs:comment "Risk Scenario is a super class for representative scenarios used for exposure accumulation, capacity monitoring, clash cover analysis etc." .

cro:ScenarioModelingAnalysis a rdfs:Class ;
    rdfs:label "Scenario Modeling Analysis" ;
    rdfs:comment "Exposure and Loss accumulation analysis based on representative scenarios. Used for  Exposure Accumulation, Capacity Monitoring, Large Loss Monitoring etc." ;
    rdfs:subClassOf cro:LossModelAnalysis .

cro:TVAR a rdfs:Class ;
    rdfs:label "Tail Value at Risk" ;
    rdfs:comment "A risk measure that quantifies the expected loss exceeding a specified Value at Risk (VaR) level. Essentially, it provides a measure of the average severity of losses when those losses are already expected to be extreme" ;
    rdfs:subClassOf cro:CatModelResultRecord .

cro:YELT a rdfs:Class ;
    rdfs:label "Year Event Loss Table" ;
    rdfs:comment "Table with list of simulated year, simulated event, along with key information about each event and its associated losses (one of the key catastrophe modeling outputs)" ;
    rdfs:subClassOf cro:CatModelResultRecord .

cro:YLT a rdfs:Class ;
    rdfs:label "Year Loss Table" ;
    rdfs:comment "A year loss table (YLT) is a table that lists historical or simulated years, with financial losses for each year" ;
    rdfs:subClassOf cro:CatModelResultRecord .

geo:asDGGS a rdf:Property,
        owl:DatatypeProperty ;
    rdfs:domain geo:Geometry ;
    rdfs:isDefinedBy geo:,
        <http://www.opengis.net/spec/geosparql/1.1/req/geometry-extension/geometry-as-dggs-literal> ;
    rdfs:range geo:dggsLiteral ;
    rdfs:subPropertyOf geo:hasSerialization ;
    skos:definition "The Discrete Global Grid System (DGGS) serialization of a Geometry"@en ;
    skos:example spec11:C.1.2.4 ;
    skos:prefLabel "as DGGS"@en .

geo:asGML a rdf:Property,
        owl:DatatypeProperty ;
    rdfs:domain geo:Geometry ;
    rdfs:isDefinedBy geo:,
        <http://www.opengis.net/spec/geosparql/1.0/req/geometry-extension/geometry-as-gml-literal>,
        <http://www.opengis.net/spec/geosparql/1.1/req/geometry-extension/geometry-as-gml-literal> ;
    rdfs:range geo:gmlLiteral ;
    rdfs:subPropertyOf geo:hasSerialization ;
    skos:definition "The GML serialization of a Geometry"@en ;
    skos:example spec11:C.1.2.4 ;
    skos:prefLabel "as GML"@en .

geo:asGeoJSON a rdf:Property,
        owl:DatatypeProperty ;
    rdfs:domain geo:Geometry ;
    rdfs:isDefinedBy geo:,
        <http://www.opengis.net/spec/geosparql/1.1/req/geometry-extension/geometry-as-geojson-literal> ;
    rdfs:range geo:geoJSONLiteral ;
    rdfs:seeAlso <https://tools.ietf.org/html/rfc7946> ;
    rdfs:subPropertyOf geo:hasSerialization ;
    skos:definition "The GeoJSON serialization of a Geometry"@en ;
    skos:example spec11:C.1.2.4 ;
    skos:prefLabel "as GeoJSON"@en .

geo:asKML a rdf:Property,
        owl:DatatypeProperty ;
    rdfs:domain geo:Geometry ;
    rdfs:isDefinedBy geo:,
        <http://www.opengis.net/spec/geosparql/1.1/req/geometry-extension/geometry-as-kml-literal> ;
    rdfs:range geo:kmlLiteral ;
    rdfs:seeAlso <https://www.ogc.org/standards/kml> ;
    rdfs:subPropertyOf geo:hasSerialization ;
    skos:definition "The KML serialization of a Geometry"@en ;
    skos:example spec11:C.1.2.4 ;
    skos:prefLabel "as KML"@en .

geo:asWKT a rdf:Property,
        owl:DatatypeProperty ;
    rdfs:domain geo:Geometry ;
    rdfs:isDefinedBy geo:,
        <http://www.opengis.net/spec/geosparql/1.0/req/geometry-extension/geometry-as-wkt-literal>,
        <http://www.opengis.net/spec/geosparql/1.1/req/geometry-extension/geometry-as-wkt-literal> ;
    rdfs:range geo:wktLiteral ;
    rdfs:subPropertyOf geo:hasSerialization ;
    skos:definition "The WKT serialization of a Geometry"@en ;
    skos:example spec11:C.1.1.2.2,
        spec11:C.1.1.2.3,
        spec11:C.1.1.2.4,
        spec11:C.1.1.2.5,
        spec11:C.1.1.2.6,
        spec11:C.1.1.2.8,
        spec11:C.1.1.3.1,
        spec11:C.1.1.3.2,
        spec11:C.1.1.3.3,
        spec11:C.1.2.2,
        spec11:C.2.1,
        spec11:C.2.2.4 ;
    skos:prefLabel "as WKT"@en .

geo:coordinateDimension a rdf:Property,
        owl:DatatypeProperty ;
    rdfs:domain geo:Geometry ;
    rdfs:isDefinedBy geo:,
        <http://www.opengis.net/spec/geosparql/1.1/req/geometry-extension/geometry-properties> ;
    rdfs:range xsd:integer ;
    skos:definition "The number of measurements or axes needed to describe the position of this Geometry in a coordinate system."@en ;
    skos:example spec11:C.1.2.3 ;
    skos:prefLabel "coordinate dimension"@en .

geo:dimension a rdf:Property,
        owl:DatatypeProperty ;
    rdfs:domain geo:Geometry ;
    rdfs:isDefinedBy geo:,
        <http://www.opengis.net/spec/geosparql/1.1/req/geometry-extension/geometry-properties> ;
    rdfs:range xsd:integer ;
    skos:definition "The topological dimension of this geometric object, which must be less than or equal to the coordinate dimension. In non-homogeneous collections, this will return the largest topological dimension of the contained objects."@en ;
    skos:example spec11:C.1.2.3 ;
    skos:prefLabel "dimension"@en .

geo:ehContains a rdf:Property,
        owl:ObjectProperty ;
    rdfs:domain geo:SpatialObject ;
    rdfs:isDefinedBy geo:,
        <http://www.opengis.net/spec/geosparql/1.0/req/geometry-extension/eh-spatial-relations>,
        <http://www.opengis.net/spec/geosparql/1.1/req/geometry-extension/eh-spatial-relations> ;
    rdfs:range geo:SpatialObject ;
    rdfs:seeAlso <http://dbpedia.org/resource/DE-9IM> ;
    owl:subPropertyOf geo:sfContains ;
    skos:definition "States that the subject SpatialObject spatially contains the object SpatialObject. DE-9IM: T*TFF*FF*"@en ;
    skos:prefLabel "contains"@en .

geo:ehCoveredBy a rdf:Property,
        owl:ObjectProperty ;
    rdfs:domain geo:SpatialObject ;
    rdfs:isDefinedBy geo:,
        <http://www.opengis.net/spec/geosparql/1.0/req/geometry-extension/eh-spatial-relations>,
        <http://www.opengis.net/spec/geosparql/1.1/req/geometry-extension/eh-spatial-relations> ;
    rdfs:range geo:SpatialObject ;
    rdfs:seeAlso <http://dbpedia.org/resource/DE-9IM> ;
    owl:subPropertyOf geo:sfWithin ;
    skos:definition "States that the subject SpatialObject is spatially covered by the object SpatialObject. DE-9IM: TFF*TFT**"@en ;
    skos:prefLabel "covered by"@en .

geo:ehCovers a rdf:Property,
        owl:ObjectProperty ;
    rdfs:domain geo:SpatialObject ;
    rdfs:isDefinedBy geo:,
        <http://www.opengis.net/spec/geosparql/1.0/req/geometry-extension/eh-spatial-relations>,
        <http://www.opengis.net/spec/geosparql/1.1/req/geometry-extension/eh-spatial-relations> ;
    rdfs:range geo:SpatialObject ;
    rdfs:seeAlso <http://dbpedia.org/resource/DE-9IM> ;
    owl:subPropertyOf geo:sfContains ;
    skos:definition "States that the subject SpatialObject spatially covers the object SpatialObject. DE-9IM: T*TFT*FF*"@en ;
    skos:prefLabel "covers"@en .

geo:ehInside a rdf:Property,
        owl:ObjectProperty ;
    rdfs:domain geo:SpatialObject ;
    rdfs:isDefinedBy geo:,
        <http://www.opengis.net/spec/geosparql/1.0/req/geometry-extension/eh-spatial-relations>,
        <http://www.opengis.net/spec/geosparql/1.1/req/geometry-extension/eh-spatial-relations> ;
    rdfs:range geo:SpatialObject ;
    rdfs:seeAlso <http://dbpedia.org/resource/DE-9IM> ;
    owl:subPropertyOf geo:sfWithin ;
    skos:definition "States that the subject SpatialObject is spatially inside the object SpatialObject. DE-9IM: TFF*FFT**"@en ;
    skos:prefLabel "inside"@en .

geo:hasArea a rdf:Property,
        owl:ObjectProperty ;
    rdfs:domain geo:SpatialObject ;
    rdfs:isDefinedBy geo:,
        <http://www.opengis.net/spec/geosparql/1.1/req/geometry-extension/spatial-object-properties> ;
    rdfs:subPropertyOf geo:hasSize ;
    skos:definition "The area of a Spatial Object."@en ;
    skos:example spec11:C.1.1.2.4,
        spec11:C.1.2.2 ;
    skos:prefLabel "has area"@en .

geo:hasBoundingBox a rdf:Property,
        owl:ObjectProperty ;
    rdfs:domain geo:Feature ;
    rdfs:isDefinedBy geo:,
        <http://www.opengis.net/spec/geosparql/1.1/req/geometry-extension/feature-properties> ;
    rdfs:range geo:Geometry ;
    rdfs:subPropertyOf geo:hasGeometry ;
    skos:definition "The minimum or smallest bounding or enclosing box of a given Feature."@en ;
    skos:example spec11:C.1.2.2 ;
    skos:prefLabel "has bounding box"@en ;
    skos:scopeNote "The target is a Geometry that defines a rectilinear region whose edges are aligned with the axes of the coordinate reference system, which exactly contains the Feature, for example an instance of http://www.opengis.net/ont/sf#envelope."@en .

geo:hasCentroid a rdf:Property,
        owl:ObjectProperty ;
    rdfs:domain geo:Feature ;
    rdfs:isDefinedBy geo:,
        <http://www.opengis.net/spec/geosparql/1.1/req/geometry-extension/feature-properties> ;
    rdfs:range geo:Geometry ;
    rdfs:subPropertyOf geo:hasGeometry ;
    skos:definition "The arithmetic mean position of all the Geometry points of a given Feature."@en ;
    skos:example spec11:C.1.2.2 ;
    skos:prefLabel "has centroid"@en ;
    skos:scopeNote "The target Geometry shall describe a point, for example an instance of http://www.opengis.net/ont/sf#Point."@en .

geo:hasLength a rdf:Property,
        owl:ObjectProperty ;
    rdfs:domain geo:SpatialObject ;
    rdfs:isDefinedBy geo:,
        <http://www.opengis.net/spec/geosparql/1.1/req/geometry-extension/spatial-object-properties> ;
    rdfs:subPropertyOf geo:hasSize ;
    skos:definition "The length of a Spatial Object."@en ;
    skos:example spec11:C.1.1.2.7,
        spec11:C.1.2.2 ;
    skos:prefLabel "has length"@en .

geo:hasMetricArea a rdf:Property,
        owl:DatatypeProperty ;
    rdfs:domain geo:SpatialObject ;
    rdfs:isDefinedBy geo:,
        <http://www.opengis.net/spec/geosparql/1.1/req/geometry-extension/spatial-object-properties> ;
    rdfs:range xsd:double ;
    rdfs:subPropertyOf geo:hasMetricSize ;
    skos:definition "The area of a Spatial Object in square meters."@en ;
    skos:example spec11:C.1.1.2.3,
        spec11:C.1.1.2.9,
        spec11:C.1.1.3.3,
        spec11:C.1.2.2 ;
    skos:prefLabel "has area in square meters"@en .

geo:hasMetricLength a rdf:Property,
        owl:DatatypeProperty ;
    rdfs:domain geo:SpatialObject ;
    rdfs:isDefinedBy geo:,
        <http://www.opengis.net/spec/geosparql/1.1/req/geometry-extension/spatial-object-properties> ;
    rdfs:range xsd:double ;
    rdfs:subPropertyOf geo:hasMetricSize ;
    skos:definition "The length of a Spatial Object in meters."@en ;
    skos:example spec11:C.1.2.2 ;
    skos:prefLabel "has length in meters"@en .

geo:hasMetricPerimeterLength a rdf:Property,
        owl:DatatypeProperty ;
    rdfs:domain geo:SpatialObject ;
    rdfs:isDefinedBy geo:,
        <http://www.opengis.net/spec/geosparql/1.1/req/geometry-extension/spatial-object-properties> ;
    rdfs:range xsd:double ;
    rdfs:subPropertyOf geo:hasMetricSize ;
    skos:definition "The length of the perimeter of a Spatial Object in meters."@en ;
    skos:example spec11:C.1.1.1.2 ;
    skos:prefLabel "has perimeter length in meters"@en .

geo:hasMetricSpatialAccuracy a rdf:Property,
        owl:DatatypeProperty ;
    rdfs:domain geo:Geometry ;
    rdfs:isDefinedBy geo:,
        <http://www.opengis.net/spec/geosparql/1.1/req/geometry-extension/geometry-properties> ;
    rdfs:range xsd:double ;
    skos:definition "The positional accuracy of the coordinates of a Geometry in meters."@en ;
    skos:example spec11:C.1.2.3 ;
    skos:note "Spatial accuracy is applicable when a Geometry is used to represent a Feature. It is expressed as a distance that indicates the truthfullness of the positions (coordinates) that define the Geometry. In this case accuracy defines a zone surrounding each coordinate within wich the real positions are known to be. The accuracy value defines this zone as a distance from the coordinate(s) in all directions (e.g. a line, a circle or a sphere, depending on spatial dimension)."@en ;
    skos:prefLabel "has spatial accuracy in meters"@en .

geo:hasMetricSpatialResolution a rdf:Property,
        owl:DatatypeProperty ;
    rdfs:domain geo:Geometry ;
    rdfs:isDefinedBy geo:,
        <http://www.opengis.net/spec/geosparql/1.1/req/geometry-extension/geometry-properties> ;
    rdfs:range xsd:double ;
    skos:definition "The spatial resolution of a Geometry in meters."@en ;
    skos:example spec11:C.1.1.2.6,
        spec11:C.1.2.2 ;
    skos:note "Spatial resolution specifies the level of detail of a Geometry. It the smallest dinstinguishable distance between spatially adjacent coordinates."@en ;
    skos:prefLabel "has spatial resolution in meters"@en .

